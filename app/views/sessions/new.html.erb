<div class="container">
  <h1> Sign in </h1>
  <%= simple_form_for :session, url: session_path do |f| %>
    <div class="form-group">
    <%= f.input :email, label: "Email" , placeholder: 'GeniusCedric@gmail',required: false, input_html: { class: 'form-control' }%>
    </div>
    <div class="form-group">
    <%= f.input :password, label: "Password",required: false, input_html: { class: 'form-control' } %>
    </div>
    <%= f.submit 'Log In' %>
  <% end %>
</div>
